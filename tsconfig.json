{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],

  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "$lib/*": ["./src/*"]
    },
    "types": [
      "unplugin-icons/types/astro",
    ]
  }
}
