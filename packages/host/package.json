{
  "name": "host",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "build": "bun run fix && bun astro build",
    "check": "biome ci . && bun astro check",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "bun astro dev",
    "fix": "bun run lint && bun run format",
    "format": "biome format --write .",
    "lint": "biome check --apply-unsafe .",
    "preview": "bun run build && bun run serve",
    "prod": "bun run check && bun astro build",
    "serve": "node ./dist/server/entry.mjs",
    "start": "bun astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/mdx": "^3.1.7",
    "@astrojs/netlify": "^5.5.3",
    "@astrojs/node": "^8.3.4",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/solid-js": "^4.4.2",
    "@astrojs/svelte": "^5.7.1",
    "@astrojs/tailwind": "^5.1.1",
    "@builder.io/qwik": "1.9.0",
    "@qwikdev/astro": "^0.6.3",
    "astro": "^4.15.9",
    "astro-compressor": "^0.4.1",
    "astro-robots-txt": "^1.0.0",
    "interests": "workspace:*",
    "software-eng": "workspace:*",
    "solid-js": "^1.9.1",
    "svelte": "^4.2.19",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@types/bun": "^1.1.10",
    "postcss-import": "^16.1.0",
    "postcss-sort-media-queries": "^5.2.0"
  }
}
