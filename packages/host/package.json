{
  "name": "host",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "build": "yarn fix && astro build",
    "check": "yarn check.fmt && yarn check.lint && astro check",
    "check.fmt": "prettier --check .",
    "check.lint": "eslint \"**/*.{js,jsx,ts,tsx,astro}\"",
    "dev": "astro dev",
    "fix": "yarn lint && yarn fmt",
    "fmt": "prettier --write .",
    "lint": "eslint --fix \"**/*.{js,jsx,ts,tsx,astro}\"",
    "lint.dry-run": "eslint --fix-dry-run \"**/*.{js,jsx,ts,tsx,astro}\"",
    "preview": "yarn build && yarn serve",
    "prod": "yarn check && astro build",
    "serve": "node ./dist/server/entry.mjs",
    "start": "astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/node": "^8.2.5",
    "@astrojs/solid-js": "^4.3.0",
    "@astrojs/svelte": "^5.5.0",
    "@astrojs/tailwind": "^5.1.0",
    "@builder.io/qwik": "^1.5.2",
    "@qwikdev/astro": "^0.5.14",
    "astro": "^4.6.3",
    "landing": "workspace:^",
    "software-eng": "workspace:^",
    "solid-js": "^1.8.17",
    "svelte": "^4.2.18",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.34.0",
    "eslint-plugin-qwik": "^1.5.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0"
  }
}
